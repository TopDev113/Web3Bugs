{
  "scripts": {
    "prettier": "prettier --write contracts/**/*.sol",
    "clean": "rm -rf ./build && rm -rf ./typechain && rm -rf ./artifacts",
    "test": "npx hardhat test",
    "test:fast": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat test",
    "compile": "yarn prettier && hardhat compile --config hardhat.compile.config.ts && hardhat run scripts/transformArtifactFilesForTests.js",
    "typechain": "typechain --target ethers-v5 --outDir typechain",
    "typechain:web3": "typechain --target web3-v1 --outDir typechainweb3 ./artifacts/**/*.json",
    "build": "rm -rf dist && yarn compile && tsc"
  },
  "dependencies": {
    "@ethereum-waffle/chai": "^3.1.0",
