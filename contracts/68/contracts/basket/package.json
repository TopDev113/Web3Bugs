    "@pie-dao/diamond": "^1.0.0",
    "@pie-dao/proxy": "^1.0.0",
    "@types/node": "^14.11.10",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-periphery": "^1.1.0",
    "chai-as-promised": "^7.1.1",
    "ethereum-waffle": "^3.1.1",
    "ethers": "^5.5.1",
    "hardhat": "^2.7.0",
    "ts-node": "^9.0.0",
    "typechain": "^4.0.3"
  },
  "devDependencies": {
