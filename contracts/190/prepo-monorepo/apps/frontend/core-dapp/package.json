  "scripts": {
    "postinstall": "yarn generate-types && yarn mst-gql:generate",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "lingui compile",
    "generate": "yarn generate-types && yarn mst-gql:generate",
    "generate-types": "typechain --target=ethers-v5 'abi/*.json' --out-dir generated/typechain",
    "dev": "npx next",
    "build": "next build && next-sitemap",
    "test": "jest -w 1",
    "test:ci": "yarn generate && yarn test",
    "start": "next start",
    "pre-commit": "lint-staged",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:fix": "eslint . --ext .tsx,.ts --fix",
    "i18n:extract": "NODE_ENV=development lingui extract",
    "i18n:compile": "yarn i18n:extract && lingui compile",
    "mst-gql:generate": "./internals/mst-gql-generate.bash"
  },
