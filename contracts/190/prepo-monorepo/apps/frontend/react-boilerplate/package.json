{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "generate-types": "typechain --target=ethers-v5 'abi/*.json' --out-dir generated/typechain",
    "generate": "yarn generate-types && yarn mst-gql:generate",
    "postinstall": "yarn generate-types && yarn mst-gql:generate && yarn i18n:compile",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "lingui compile",
    "dev": "npx next",
    "build": "next build && next-sitemap",
    "test": "jest -w 1",
    "test:ci": "yarn generate && yarn test",
    "i18n:extract": "NODE_ENV=development lingui extract --locale en",
    "i18n:compile": "yarn i18n:extract && lingui compile",
    "start": "next start",
    "lint": "eslint . --max-warnings 0 --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "mst-gql:generate": "./internals/mst-gql-generate.bash"
  },
  "dependencies": {
    "@lingui/react": "3.14.0",
    "@lingui/core": "3.14.0",
    "@lingui/macro": "3.14.0",
    "@sentry/nextjs": "6.19.6",
    "antd": "4.21.6",
    "clone-deep": "4.0.1",
    "configcat-js-ssr": "3.10.0",
    "ethereum-multicall": "2.10.0",
    "ethers": "5.6.9",
    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "mobx": "6.6.1",
    "mobx-react-lite": "3.4.0",
    "mobx-state-tree": "5.1.6",
    "mst-gql": "0.17.1",
    "next": "12.1.6",
    "next-sitemap": "2.5.19",
    "prepo-constants": "*",
    "prepo-stores": "*",
    "prepo-utils": "*",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-jazzicon": "1.0.3",
    "react-use": "17.3.2",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@lingui/cli": "3.14.0",
    "@lingui/loader": "3.14.0",
    "@typechain/ethers-v5": "10.0.0",
    "@types/react": "17.0.45",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/styled-components": "5.1.25",
    "babel-plugin-macros": "3.1.0",
    "config": "*",
    "next-transpile-modules": "9.0.0",
    "tsconfig": "*",
    "typechain": "8.0.0"
  },
  "resolutions": {
    "@types/react": "17.0.45"
  }
}
